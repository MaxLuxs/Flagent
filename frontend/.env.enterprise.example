# Flagent Frontend - Enterprise Edition Configuration

# ==================================================
# EDITION
# ==================================================
ENV_EDITION=enterprise

# ==================================================
# API CONFIGURATION
# ==================================================
ENV_API_BASE_URL=http://localhost:18000
ENV_API_TIMEOUT=30000

# ==================================================
# DEBUG & LOGGING
# ==================================================
ENV_DEBUG_MODE=false
ENV_ERROR_TRACKING=true

# ==================================================
# LOCALIZATION
# ==================================================
ENV_DEFAULT_LANGUAGE=en

# ==================================================
# CORE FEATURES
# ==================================================
ENV_FEATURE_AUTH=true
ENV_FEATURE_REALTIME=true

# ==================================================
# OPEN SOURCE FEATURES (Always Available)
# ==================================================
# Metrics & Analytics
ENV_FEATURE_METRICS=true

# AI-Powered Smart Rollout
ENV_FEATURE_SMART_ROLLOUT=true

# Anomaly Detection
ENV_FEATURE_ANOMALY_DETECTION=true

# ==================================================
# ENTERPRISE-ONLY FEATURES
# ==================================================
# Multi-Tenancy (organizations, workspaces)
ENV_FEATURE_MULTI_TENANCY=true

# SSO (OAuth, OIDC, SAML)
ENV_FEATURE_SSO=true

# Billing & Subscriptions (Stripe)
ENV_FEATURE_BILLING=true

# Slack Integration (notifications, alerts)
ENV_FEATURE_SLACK=true

# Advanced Analytics (detailed metrics, custom reports)
ENV_FEATURE_ADVANCED_ANALYTICS=true

# Audit Logs (compliance, security)
ENV_FEATURE_AUDIT_LOGS=true

# Role-Based Access Control
ENV_FEATURE_RBAC=true
